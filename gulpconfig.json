{
    "wabDevelopmentPath": [
        "./WebAppBuilder/**",
        "!./WebAppBuilder/**/*.js.map",
        "!./WebAppBuilder/**/*.ts",
        "!./WebAppBuilder/**/*.md"
    ],
    "wabDeploymentPaths": [
        "./docs/2d/widgets"
    ],
    "watchFileTypesNeedBuild": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./docs/**"
    ],
    "watchFileTypesNoBuild": [
        "./**/*.html",
        "./**/*.css",
        "./**/*.json",
        "./**/*.html",
        "./**/strings.js",
        "!./docs/**"
    ]
}